{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ], 
         "urls" : [
           "http://localhost:4200/*",
           "http://127.0.0.1:8080/*"
         ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
    "dataGroups": [
      {
        "name": "api",
        "urls": [
          "http://localhost:3000/api/*"
        ],
        "cacheConfig": {
          "strategy" : "performance",
          "maxAge": "15m",
          "timeout": "10s",
          "maxSize": 20
        }
      }
    ]
}
